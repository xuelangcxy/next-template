{
  "name": "next-test",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:pm2": "appDir=`pwd` && cd ~ && pm2 ls && cd $appDir",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "pm2 start ecosystem.config.js --only test-dev",
    "production": "npm run start:pm2 && npx cross-env API_ENV=production pm2 startOrReload ecosystem.config.js --only test",
    "build": "npx cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" NODE_ENV=production API_ENV=production npx next build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "yarn && lint-staged",
      "commit-msg": "npx commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "resolutions": {
    "css-loader": "1.0.0"
  },
  "lint-staged": {
    "*.less": [
      "npx prettier --parser less --write",
      "npx stylelint",
      "git add"
    ],
    "*.tsx": [
      "npx stylelint",
      "npx eslint --fix",
      "git add"
    ],
    "*.ts": [
      "npx prettier --write",
      "npx eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/register": "7.4.4",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-less": "^1.0.1",
    "antd-mobile": "2.3.1",
    "babel-plugin-import": "1.11.0",
    "cross-env": "5.2.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-html": "^6.1.2",
    "http-proxy-middleware": "0.19.1",
    "koa": "2.6.1",
    "koa-body": "4.1.0",
    "koa-router": "7.1.0",
    "koa2-connect": "1.0.2",
    "less": "^3.10.3",
    "less-plugin-functions": "^1.0.0",
    "moment": "2.23.0",
    "next": "9.0.8-canary.5",
    "nprogress": "0.2.0",
    "null-loader": "^3.0.0",
    "path-to-regexp": "2.4.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "styled-jsx-plugin-less": "0.0.1-alpha.2",
    "styled-jsx-plugin-postcss": "2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@commitlint/cli": "7.2.1",
    "@commitlint/config-conventional": "7.1.2",
    "@commitlint/resolve-extends": "7.1.2",
    "babel-eslint": "8.2.6",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "eslint": "5.9.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.11.1",
    "file-loader": "^5.0.2",
    "husky": "1.2.0",
    "lint-staged": "8.1.0",
    "style-loader": "^1.1.3",
    "stylelint": "9.8.0",
    "stylelint-config-css-modules": "1.3.0",
    "stylelint-config-standard": "18.2.0",
    "url-loader": "1.1.2",
    "webpack": "^4.41.5",
